        slugCallback: function(filename) {
            return filename.replace('(', '_').replace(']', '_');
        }
    });
    
    $('#btnCategory').click(function() {
        var modalElem = $('#modalSlideUp');
            modalElem.modal('show');
            modalElem.children('.modal-dialog').removeClass('modal-lg');
    });



// Category

        [% PROCESS admin/content/product/megamenu.html.tt %]
    
        $("#check-tree").dynatree({
            checkbox: true,
            selectMode: 2,
            children: treeData,
            onSelect: function(select, node) {
                // Display list of selected nodes
                var selNodes = node.tree.getSelectedNodes();
                // convert to title/key array
                var selKeys = $.map(selNodes, function(node) {
                    //return "[" + node.data.key + "]: '" + node.data.title + "'";
                    console.log( node.data );
                    return node.data.path_name;
                });
                $("#ProductCategory").html(selKeys.join("<br>"));
            },
            onClick: function(node, event) {
                // We should not toggle, if target was "checkbox", because this
                // would result in double-toggle (i.e. no toggle)
                if (node.getEventTargetType(event) == "title")
                    node.toggleSelect();
            },
            onKeydown: function(node, event) {
                if (event.which == 32) {
                    node.toggleSelect();
                    return false;
                }
            },
            cookieId: "dynatree-Cb2",
            idPrefix: "dynatree-Cb2-2"
        });
        
        [% IF hilt.admin.product_item.category_id %]
          $("#check-tree").dynatree("getTree").selectKey("[% hilt.admin.product_item.category_id %]");
        [% END %]
    
    
});
</script>
    
